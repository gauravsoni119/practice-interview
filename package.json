{
  "name": "practice-leetcode",
  "homepage": "https://gauravsoni119.github.io/practice-interview/",
  "version": "1.0.0",
  "description": "A package contains List of data structures and algorithms commonly asked during the interview",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "docs": "npm run docs:build && npm run docs:view",
    "docs:build": "tsc --project tsconfig.json && jsdoc -c jsdoc.config.json",
    "docs:view": "live-server docs --port=4000",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "format:check": "prettier --config .prettierrc 'src/**/*.ts' --check",
    "predeploy": "npm run docs:build",
    "deploy": "gh-pages -d docs"
  },
  "keywords": [
    "problem",
    "solving",
    "algorithms",
    "data",
    "structure",
    "array",
    "linked",
    "list",
    "stack",
    "queue",
    "tree",
    "graphs",
    "dynamic",
    "programing"
  ],
  "author": "Gaurav Soni",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "gh-pages": "^4.0.0",
    "jest": "^28.1.3",
    "jsdoc": "^3.6.11",
    "live-server": "^1.2.2",
    "minami": "^1.2.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
