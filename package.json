{
  "name": "practice-leetcode",
  "version": "1.0.0",
  "description": "A package contains the solution of the leetcode problems",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "docs": "npm run docs:build && npm run docs:view",
    "docs:build": "tsc --project tsconfig.json && jsdoc -c jsdoc.config.json",
    "docs:view": "live-server docs --port=4000",
    "test": "jest"
  },
  "keywords": [
    "problem",
    "solving",
    "algorithms",
    "data",
    "structure",
    "array",
    "linked",
    "list",
    "stack",
    "queue",
    "tree",
    "graphs",
    "dynamic",
    "programing"
  ],
  "author": "Gaurav Soni",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.8",
    "jest": "^28.1.3",
    "jsdoc": "^3.6.11",
    "live-server": "^1.2.2",
    "minami": "^1.2.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
